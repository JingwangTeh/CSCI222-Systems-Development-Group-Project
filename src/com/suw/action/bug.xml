<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
    
<struts>

	<package name="bug" namespace="/bug" extends="struts-default">  
	 
	 	<action name="search" class="com.suw.action.BugAction" method="search">
	 		<result name="success" >/result.jsp</result>
	 	</action>
	 	
	 	<action name="scan" class="com.suw.action.BugAction" method="scan">
	 		<result name="success">/viewBugDetail.jsp</result>
	 	</action>
	 	
	 	<action name="add" class="com.suw.action.BugAction" method="add">
	 		<result name="success">/home.jsp</result>
	 	</action>
	 	
	 	<action name="merge" class="com.suw.action.BugAction" method="merge">
	 		<result name="success">/mergeDup.jsp</result>
	 	</action>
	 	
	 	<action name="assign" class="com.suw.action.BugAction" method="assign">
	 		<result name="success">/home.jsp</result>
	 	</action>
	 	
 	</package>
	 
</struts>